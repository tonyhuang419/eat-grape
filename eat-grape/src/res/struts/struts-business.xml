<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd" >
<struts>
	<package name="distinct" namespace="/business/distcommmgr/distinct"
		extends="default">
		<action name="*" class="Distinct" method="{1}">
			<result name="showIndex">
				/WEB-INF/jsp/distinct/distinct.jsp
      		</result>
			<result name="showAdd">
				/WEB-INF/jsp/distinct/add.jsp
      		</result>
			<result name="showUpdate">
				/WEB-INF/jsp/distinct/update.jsp
      		</result>
		</action>
	</package>
</struts>