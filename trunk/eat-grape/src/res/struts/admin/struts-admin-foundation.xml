<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
  <package extends="default" name="district" namespace="/foundation/place/district">
    <action name="*" class="com.eatle.web.action.backend.foundation.place.DistrictAction" method="{1}">
			<result name="showIndex">/WEB-INF/jsp/foundation/place/district/district.jsp</result>
			<result name="showAdd">/WEB-INF/jsp/foundation/place/district/add.jsp</result>
			<result name="showUpdate">/WEB-INF/jsp/foundation/place/district/update.jsp</result>
			<result name="showSetSubDist">/WEB-INF/jsp/foundation/place/district/showSetSubDist.jsp</result>
	</action>
  </package>
  <package extends="default" name="shool" namespace="/foundation/place/shool">
    <action class="com.eatle.web.action.backend.foundation.place.ShoolAction" method="{1}" name="*">
      <result name="showIndex">
        /WEB-INF/jsp/foundation/place/shool/shool.jsp
      </result>
      <result name="showAdd">
        /WEB-INF/jsp/foundation/place/shool/add.jsp
      </result>
      <result name="showUpdate">
        /WEB-INF/jsp/foundation/place/shool/update.jsp
      </result>
    </action>
  </package>
  <package extends="default" name="community" namespace="/foundation/place/community">
    <action  name="*" class="com.eatle.web.action.backend.foundation.place.CommunityAction" method="{1}">
      <result name="showIndex">
        /WEB-INF/jsp/foundation/place/community/community.jsp
      </result>
      <result name="showAdd">
        /WEB-INF/jsp/foundation/place/community/add.jsp
      </result>
      <result name="showUpdate">
        /WEB-INF/jsp/foundation/place/community/update.jsp
      </result>
    </action>
  </package>
</struts>